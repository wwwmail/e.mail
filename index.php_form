function uprav($data)
{
	return str_replace("\\n", "<br/>", $data);
}

function cislo($cislo, $delka = 0, $des = true, $zaporne=false)
{
	$cislo = trim(strtr($cislo, ",", "."));
	$cislo = trim(strtr($cislo, "+", ""));
	if ($delka != 0 && strlen($cislo) != $delka) {
		return false;
	}
	$oddel = 0;
	for ($i = 0; $i < strlen($cislo); $i++) {
		if ($zaporne && $i==0 && $cislo[$i]=="-") {
			continue;
		}
		if ($cislo[$i] == '.') {
			$oddel++;
		} elseif ($cislo[$i] < '0' || $cislo[$i] > '9') {
			return false;
		}
	}
	if ($oddel > 1 ||
		($oddel != 0 && !$des &&
	(substr($cislo, 0, strpos($cislo, ".")) != $cislo))) {
		return false;
	}
	return true;
}
function getAmountInSmallest($data){
	//echo "data: $data - ";
	$data = trim(strtr($data, ",", "."));
	$pos = strpos($data, ".");
	if ($pos !== false){
		//echo " -uprava- ";
		$a = substr($data, 0, $pos);
		$b = substr($data, $pos+1);
		while (strlen($b)<2) {
			$b.="0";
		}
		$data = ($a.$b)*1;
	}else{
		//echo " -nasobeni- ";
		$data=$data*100;
	}
	//echo "$data<br/>";
	return $data;
}
